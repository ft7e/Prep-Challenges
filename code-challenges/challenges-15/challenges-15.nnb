        {
            "language": "javascript",
            "source": [
                "function equalizeArray(arr) {
    // Write your code here
    let maxCounter = 0;
    
    
    for (let j = 0; j < arr.length; j++){
        let counter = 1;
        for(let i = j+1; i < arr.length; i++){
            if (arr[j] == arr[i]) counter++;
        }
        if (counter > maxCounter) maxCounter = counter;
    }
    return arr.length - maxCounter;
}"
