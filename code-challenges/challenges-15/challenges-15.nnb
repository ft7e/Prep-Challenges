        {
            "language": "javascript",
            "source": [
                "function howManyGames(k, d, m, s) {

  let totalCost = k;
  let gameCount = 0;
  
  while(totalCost <= s){
    k = k - d;
    if(k <= m){
      totalCost = totalCost + m;
      gameCount++;
    }else{
      totalCost = totalCost + k;
      gameCount++;
    }
    
  }
  
  return gameCount;
}"
